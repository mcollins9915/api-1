<%- include('../partials/header'); %>
<main class="main main-colors container">
    <h2 class="main-h2"><%= name %></h2>
    <section class="section section-colors">
        <div class="row">
            <% data.forEach(item => { %>
            <div class="col-md-4">
                <a href="/characters/<%= item.id %>">
                    <img class="img-fluid" src="/media/S3.jpg" alt="simpsonsimg">
            </a>
                <figcaption class="figcap figcap-colors"><%= item.name %></figcaption>
            </div>
            
            
            <% }) %>
        </div>
    </section>
</main>

<%- include('../partials/footer'); %>